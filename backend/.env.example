# Supabase Database URL (use POSTGRES_URL from Vercel/Supabase integration)
DATABASE_URL="postgresql://username:password@host:5432/postgres"

# Supabase Storage credentials (use from Vercel/Supabase integration)
SUPABASE_URL="https://your-project.supabase.co"
SUPABASE_SERVICE_ROLE_KEY="your-service-role-key"

# JWT Secret for authentication (generate a secure random string for production)
JWT_SECRET="your-super-secret-jwt-key-change-this-in-production"

# Server Port
PORT=3000

# Node Environment
NODE_ENV=development

# Frontend URL for QR code generation
# Local: http://localhost:5173
# Production: https://wooftrace.com
FRONTEND_URL=http://localhost:5173
